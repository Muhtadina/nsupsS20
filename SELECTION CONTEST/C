#include <bits/stdc++.h>
using namespace std;
int main()
{
    int N; cin >> N;
    bool d = false;
    for(int i=1; i<=9; i++)
    {
        if(N%i==0 && N/i<=9)
        {
            d=true;
            break;
        }
    }
    if(d)
        cout<<"Yes";
    else
        cout << "No";
    return 0;
}
